// Print users
Private aUsers
  aResult := DoSql( "Select host,user From user" )
  if aResult[1] == 0
     if OpenReport( "userprn.rpt","users" )
        PrintReport()
        HWG_MSGINFO( "Done!","Script result" )
     else
        HWG_MsgStop( "Can't open report" )
     endif
  else
     HWG_MsgStop( "Error executing query" )
  endif